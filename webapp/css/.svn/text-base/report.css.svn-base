.reportTitle{ 	
	font-size: 24px; 
	font-weight: bold;
	text-align: center;
	padding: 10px;	
}

.reportTitleSub{
	text-align: center;
	padding: 0px 0px 10px 0px;	
}

.reportContent{
	text-align: center;
	padding: 0px 10px 20px 10px;	
}

.reportDetailContent{
	text-align: center;
	padding: 0px 10px 10px 10px;
}


.contentTableSlim {
    padding: 0;
    margin: 0 auto;  
    border-collapse:collapse;
    //width: 100%;
}

.contentTableSlim td {
    border: 1px solid #C1DAD7;  
    background: #fff;
    font-size:12px;   
    color: #4f6b72;
    padding: 2px;
}

.contentTableSlim a {    
    color: red;   
}

.contentTableSlim th {
    border: 1px solid #C1DAD7;  
    background: #fff;
    font-size:12px;
    color: #4f6b72;
    padding: 2px;
    white-space: nowrap
}

#reportGraph {
  width: 90%;
  height: 200px;
  margin: 0px auto 10px auto;
}

.chartDiv {
    border:1px solid #ccc;
    margin:0px 50px 10px 50px;
    background-color:#fff;
    text-align: center;
}

.chartTitle {
    font-size:14px;
    font-weight:bold;
    padding:5px 0px 0px 0px;
}